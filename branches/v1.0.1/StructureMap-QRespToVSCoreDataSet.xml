  <group>
    <name value="QRespToHeader"/>
    <typeMode value="none"/>
    <input>
      <name value="qr"/>
      <mode value="source"/>
    </input>
    <input>
      <name value="ddcc"/>
      <mode value="target"/>
    </input>
    <rule>
      <name value="header items"/>
      <source>
        <context value="qr"/>
