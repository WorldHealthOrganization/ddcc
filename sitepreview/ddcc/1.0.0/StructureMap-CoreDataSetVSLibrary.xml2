    <span style="color: navy">}</span> <i>&quot;create immunization recommendation resource&quot;</i><span style="color: navy">;</span>
  <span style="color: navy">}</span> <i>&quot;set uuids&quot;</i><span style="color: navy">;</span>
<span style="color: navy">}

