        <xpath value="count(*[self::snapshot or self::differential]/f:element[not(contains(f:path/@value, '.')) and f:sliceName])=0"/>
        <source value="http://hl7.org/fhir/StructureDefinition/StructureDefinition"/>
      </constraint>
